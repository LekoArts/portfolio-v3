---
import ArtImage from '@components/art/art-image.astro'
import { getCollection } from 'astro:content'

const photos = await getCollection('artDesign')
---

{
	photos
		.sort((a, b) => b.data.date_taken!.getTime() - a.data.date_taken!.getTime())
		.map((photo) => <ArtImage imageUrls={photo.data.imageUrls} alt={photo.data.description} />)
}
