---
import Box, { type BoxProps } from '@components/primitives/box.astro'
import { proseRootStyle } from './prose.css'
import { expressiveCodeRootStyle } from './expressive-code.css'

interface Props extends BoxProps<'div' | 'article'> {}

const { class: className, ...rest } = Astro.props
---

<Box class:list={[proseRootStyle, expressiveCodeRootStyle, className]} {...rest}><slot /></Box>
